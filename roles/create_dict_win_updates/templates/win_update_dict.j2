{% for win_update in list_of_updates.updates | dict2items %}
   {
     "update": {
       "kb": [
         "{{ win_update.value.kb[0] }}"
       ],
       "hostname: "{{inventory_hostname }}",
       "installed": false,
       "id": "{{ win_update.value.id }}",
       "title": "{{ win_update.value.title }}"
     }
   }{{ "," if not loop.last else ' '}}
{% endfor %}   